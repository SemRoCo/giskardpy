<launch>

    <include file="$(find iai_dlr_rollin_justin)/launch/justin_upload.launch"/>

    <node pkg="giskardpy_ros" type="justin_standalone.py" name="giskard" output="screen"/>

    <node pkg="giskardpy_ros" type="interactive_marker.py" name="giskard_interactive_marker" output="screen">
        <rosparam param="enable_self_collision">False</rosparam>
        <rosparam param="marker_scale">1</rosparam>
        <rosparam param="interactive_marker_chains">
<!--          - [map, r_gripper_tool_frame]-->
<!--          - [map, l_gripper_tool_frame]-->
          - [base_footprint, torso4]
<!--          - [map, base_footprint]-->
<!--          - [map, high_def_frame]-->
        </rosparam>
    </node>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find giskardpy_ros)/launch/rviz_config/standalone.rviz" />

</launch>

