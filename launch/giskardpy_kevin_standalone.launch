<launch>


    <node pkg="giskardpy" type="kevin_standalone.py" name="giskard" output="screen"/>

    <node pkg="giskardpy" type="interactive_marker.py" name="giskard_interactive_marker" output="screen">
        <rosparam param="disable_self_collision">False</rosparam>
        <rosparam param="interactive_marker_chains">
            - [map, robot_arm_tool_link]
            - [map, l_gripper_tool_frame]
        </rosparam>
    </node>
</launch>